<!--
Feb 18, 2017 8:51:56 AM hudson.model.UpdateCenter$DownloadJob run INFO: Installation successful: PAM Authentication plugin
-->
<decoder name="java-logger">
<prematch>^\w+ \d+, \d+ \d+:\d+:\d+ \wM \S+ \S+ \w+: </prematch>
</decoder>

<decoder name="java-logger-generic">
    <parent>java-logger</parent>
    <regex>^(\w+ \d+, \d+ \d+:\d+:\d+ \wM) \S+ (\S+) (\w+): (\.+)</regex>
    <order>timestamp, component, severity, msg</order>
</decoder>
